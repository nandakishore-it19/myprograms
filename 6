#include<stdio.h>
void main()
{
int a,b,c,d;
printf("Enter your 1angle");
scanf("%d",&a);
printf("Enter your 2angle");
scanf("%d",&b);
printf("Enter your 3angle");
scanf("%d",&c);
if (a!=0&&b!=0&&c!=0)
{
if (a+b+c==180)
{
printf("it is a valid triangle");
}
else
{
printf("it is not a valid triangle");
}
}
else
printf("it is not a valid triangle");
}
